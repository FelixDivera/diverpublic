  "paths": {
    "/pull/vehicle-status": {
      "get": {
        "tags": [ "Daten/ Informationen" ],
        "summary": "Daten/ Infos zu allen Informationen abrufen",
        "consumes": [ "application/json" ],
        "security": [ { "System-Benutzer Accesskey": [] } ],
        "responses": {
          "200": {
            "description": "successful"
          },
          "403": {
            "description": "Accesskey fehlt oder falsch!"
          }
        }
      }
    }
  }
